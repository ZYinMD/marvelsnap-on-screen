{
  "explorer.sortOrder": "filesFirst", // good for file system based routing
  "files.exclude": {
    "**/node_modules": true,
    "**/~$*.xlsx": true
  },
  // note: search exclude inherit files exlude, unless you you override them.
  "search.exclude": {
    "**/.svelte-kit": true,
    "**/deprecated": true,
    "**/.vercel/output": true
  },
  "deno.enablePaths": ["facts"],
  "svelte.plugin.svelte.compilerWarnings": {
    "a11y-click-events-have-key-events": "ignore",
    "a11y-no-static-element-interactions": "ignore"
