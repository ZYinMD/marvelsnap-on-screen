{
  "name": "ui",
  "version": "1.0.108",
  "scripts": {
    "dev": "vite dev",
    "format": "prettier . --write",
    "lint": "prettier . --check . && eslint .",
    "test": "vitest",
    "check": "bash svelte-check.sh",
    "check-all": "svelte-kit sync && pnpm format && pnpm lint && pnpm test run && pnpm check",
    "svelte-check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "build": "pnpm check-all && vite build",
    "preview": "vite build && vite preview",
    "version": "bash version.sh"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.15.2",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vitest": "^1.6.0"
  },
  "type": "module",
  "dependencies": {
    "@vercel/analytics": "^1.4.1",
    "highlight-words": "^1.2.2",
    "workbox-core": "^7.3.0",
    "workbox-recipes": "^7.3.0"
  }
}
