{
  "name": "ui",
  "version": "0.0.4",
  "scripts": {
    "dev": "vite dev",
    "format": "prettier . --write",
    "lint": "prettier . --check . && eslint .",
    "test": "vitest",
    "check-all": "svelte-kit sync && pnpm format && pnpm lint && pnpm test run && svelte-check --fail-on-warnings --tsconfig ./tsconfig.json",
    "svelte-check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "build": "pnpm check-all && vite build",
    "preview": "vite build && vite preview",
    "version": "bash version.sh"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.23.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.33.0",
    "prettier": "^3.0.2",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.2",
    "vitest": "^0.34.2"
  },
  "type": "module"
}
