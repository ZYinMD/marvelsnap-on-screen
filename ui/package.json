{
  "name": "ui",
  "version": "1.0.13",
  "scripts": {
    "dev": "vite dev",
    "format": "prettier . --write",
    "lint": "prettier . --check . && eslint .",
    "test": "vitest",
    "check": "bash svelte-check.sh",
    "check-all": "svelte-kit sync && pnpm format && pnpm lint && pnpm test run && pnpm check",
    "svelte-check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "build": "pnpm check-all && vite build",
    "preview": "vite build && vite preview",
    "version": "bash version.sh"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.1",
    "@sveltejs/kit": "^2.0.6",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.1"
  },
  "type": "module",
  "dependencies": {
    "@vercel/analytics": "^1.1.1",
    "highlight-words": "^1.2.2",
    "workbox-core": "^7.0.0",
    "workbox-recipes": "^7.0.0"
  }
}
